= form_for @family_card do |f|
  = f.error_messages
  %p
    = f.label :parent_first_name
    %br
    = f.text_field :parent_first_name
  %p
    = f.label :parent_last_name
    %br
    = f.text_field :parent_last_name
  %p
    = f.label :student_name
    %br
    = f.text_field :student_name
  %p
    = f.label :phone
    %br
    = f.text_field :phone
  %p
    = f.label :email
    %br
    = f.text_field :email
  %p
    = f.label :address1
    %br
    = f.text_field :address1
  %p
    = f.label :address2
    %br
    = f.text_field :address2
  %p
    = f.label :city
    %br
    = f.text_field :city
  %p
    = f.label :state
    %br
    = f.text_field :state
  %p
    = f.label :zipcode
    %br
    = f.text_field :zipcode
  %p
    = f.label :primary_parent_id
    %br
    = f.number_field :primary_parent_id
  %p
    = f.submit
