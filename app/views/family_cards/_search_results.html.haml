- if @family_cards.empty?
  .alert.fade.in.info{ "data-alert" => "alert" }
    %button.close{ :type => "button", :'data-dismiss' => "alert"} Ã—
    %p
      No prospects were found.
      = link_to "Try another search?", search_family_cards_path
- @family_cards.each do |family_card|
  %h2 Found:
  %dl.dl-horizontal.contact.span4
    %dt Parent/Guardian Name:
    %dd= family_card.default_parent.name
    %dt Student Name:
    %dd= family_card.student_name
    %dt Phone:
    %dd= family_card.phone
    %dt Email:
    %dd= family_card.email
  %dl.dl-horizontal.address.span4
    %dt Address:
    %dd
      = family_card.address1
      %br/
      - if family_card.address2
        = family_card.address2
        %br/
      = "#{family_card.city}, #{family_card.state}  #{family_card.zip_code}"
